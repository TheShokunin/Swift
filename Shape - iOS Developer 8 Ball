let playerName = "Oli"
let playerQuestion = "Should Shape take a chance on Oli as their new Student iOS Developer?"
let randomNumber = Int.random(in:1...9)
let eightBall: String
switch randomNumber {
  case 1:
  eightBall = "Absolutely - he will not let you down!"
  case 2:
  eightBall = "Selvf√∏lgelig!"
  case 3:
  eightBall = "Have you noticed there are no responses that say no? :-D"
  case 4:
  eightBall = "For sure!"
  case 5:
  eightBall = "He may be early in his learnings, but he is incredibly passionate and hungry to succeed!"
  case 6:
  eightBall = "Darth Vader. Oops, sorry, wrong quiz!"
  case 7:
  eightBall = "How can we say no??"
  case 8:
  eightBall = "Yes!"
  case 9:
  eightBall = "Definitely!"
  default:
  eightBall = "Error"
}
print("\(playerName) asks: \(playerQuestion)")
print("Magic 8 Ball's answer: \(eightBall)")
